    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
// A simple algorithm: count every ,;/ and tab, see which one is used most often
//
var delimiters = [ ',', ';', '/', '\\t' ];
var counters = new Array();
var i;
for (i=0;i&lt;line.length;i++) {
  var c=line.charAt(i);
  for (d=0;d&lt;delimiters.length;d++) {
    if (c==delimiters[d]) {
      if (counters[c]==null) {
	    counters[c]=1;
      } else {
        counters[c]++;
      }
    }
  }
}
var maxC=0;
var max=0;
for (i=0;i&lt;delimiters.length;i++) {
  if (counters[delimiters[i]]&gt;max) {
    maxC=i;
    max=counters[delimiters[i]];
  }
}

// Alert( "counters["+delimiters[maxC]+"] = "+counters[delimiters[maxC]] );

var delimiter=""+delimiters[maxC];
var count = counters[delimiters[maxC]];


</jsScript_script>
      </jsScript>
    </jsScripts>
