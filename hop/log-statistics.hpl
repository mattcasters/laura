    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
var lastSlashIndex = filename.lastIndexOf("/");
if (lastSlashIndex&lt;0) {
  lastSlashIndex = filename.lastIndexOf("\\");
}

var shortName = substr(filename, lastSlashIndex+1 );

var dashIndex = indexOf( shortName, "-" );

if (dashIndex&lt;0) {
  throw "Dash expected in count filename: "+shortName;
}

var label = substr(shortName, 0, dashIndex);
</jsScript_script>
      </jsScript>
    </jsScripts>
